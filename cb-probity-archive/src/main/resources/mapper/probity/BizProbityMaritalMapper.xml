<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cb.probity.mapper.BizProbityMaritalMapper">

    <resultMap type="BizProbityMarital" id="BizProbityMaritalResult">
        <result property="id" column="id"/>
        <result property="probityId" column="probity_id"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
        <result property="maritalStatus" column="marital_status"/>
        <result property="linkUserPhone" column="link_user_phone"/>
    </resultMap>

    <sql id="selectBizProbityMaritalVo">
        select id,
               probity_id,
               create_by,
               create_time,
               update_by,
               update_time,
               remark,
               marital_status,
               link_user_phone
        from biz_probity_marital
    </sql>

    <select id="selectBizProbityMaritalList" parameterType="BizProbityMarital" resultMap="BizProbityMaritalResult">
        <include refid="selectBizProbityMaritalVo"/>
        <where>
            <if test="probityId != null  and probityId != ''">and probity_id = #{probityId}</if>
            <if test="maritalStatus != null  and maritalStatus != ''">and marital_status = #{maritalStatus}</if>
            <if test="linkUserPhone != null  and linkUserPhone != ''">and link_user_phone = #{linkUserPhone}</if>
            <!-- 数据范围过滤 -->
            ${params.dataScope}
        </where>
    </select>

    <select id="selectBizProbityMaritalById" parameterType="String" resultMap="BizProbityMaritalResult">
        <include refid="selectBizProbityMaritalVo"/>
        where id = #{id}
    </select>

    <insert id="insertBizProbityMarital" parameterType="BizProbityMarital">
        insert into biz_probity_marital
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null and id != ''">id,</if>
            <if test="probityId != null">probity_id,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="maritalStatus != null">marital_status,</if>
            <if test="linkUserPhone != null">link_user_phone,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null and id != ''">#{id},</if>
            <if test="probityId != null">#{probityId},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="maritalStatus != null">#{maritalStatus},</if>
            <if test="linkUserPhone != null">#{linkUserPhone},</if>
        </trim>
    </insert>

    <insert id="insertBatch" keyProperty="id">
        insert into biz_probity_marital
        <trim prefix="(" suffix=")" suffixOverrides=",">
            id,
            probity_id,
            create_by,
            create_time,
            update_by,
            update_time,
            remark,
            marital_status,
            link_user_phone,
        </trim>
        values
        <foreach collection="entities" item="entity" separator=",">
            <trim prefix="(" suffix=")" suffixOverrides=",">
                #{entity.id},
                #{entity.probityId},
                #{entity.createBy},
                #{entity.createTime},
                #{entity.updateBy},
                #{entity.updateTime},
                #{entity.remark},
                #{entity.maritalStatus},
                #{entity.linkUserPhone},
            </trim>
        </foreach>
    </insert>

    <update id="updateBizProbityMarital" parameterType="BizProbityMarital">
        update biz_probity_marital
        <trim prefix="SET" suffixOverrides=",">
            <if test="probityId != null">probity_id = #{probityId},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="maritalStatus != null">marital_status = #{maritalStatus},</if>
            <if test="linkUserPhone != null">link_user_phone = #{linkUserPhone},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBizProbityMaritalById" parameterType="String">
        delete
        from biz_probity_marital
        where id = #{id}
    </delete>

    <delete id="deleteBizProbityMaritalByIds" parameterType="String">
        delete from biz_probity_marital where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>