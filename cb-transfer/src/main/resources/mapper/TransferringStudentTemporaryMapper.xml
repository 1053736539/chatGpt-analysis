            <if test="position != null  and position != ''"> and position = #{position}</if>
            <if test="beginTime != null  and beginTime != ''"> and begin_time &gt;= #{beginTime}</if>
            <if test="endTime != null  and endTime != ''"> and begin_time &lt;= #{endTime}</if>
            <if test="addr != null  and addr != ''"> and addr = #{addr}</if>
            <if test="attach != null  and attach != ''"> and attach = #{attach}</if>
        </where>
    </select>
    
    <select id="selectTransferringStudentTemporaryById" parameterType="String" resultMap="TransferringStudentTemporaryResult">
