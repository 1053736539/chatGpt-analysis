            <if test="visitAddr != null">visit_addr = #{visitAddr},</if>
            <if test="visitContent != null">visit_content = #{visitContent},</if>
            <if test="visitUserJson != null">visit_user_json = #{visitUserJson},</if>
            <if test="visitObjJson != null">visit_obj_json = #{visitObjJson},</if>
            <if test="visitCauses != null">visit_causes = #{visitCauses},</if>

        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTransferringStudentVisitById" parameterType="String">
        delete from transferring_student_visit where id = #{id}
    </delete>

    <delete id="deleteTransferringStudentVisitByIds" parameterType="String">
        delete from transferring_student_visit where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>