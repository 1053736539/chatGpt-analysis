        <result property="delFlag"    column="del_flag"    />
        <result property="isCleaning"    column="isCleaning"    />
    </resultMap>

    <sql id="selectBasNotRentedOutSourceVo">
        select s.source_id,s.project_name, s.properties_sale, s.building, s.unit, s.room_number,
               s.monthly_rent, s.area_id, s.import_time, s.create_time, s.update_time, s.create_by,
               s.update_by, s.del_flag,
            case when o.source_id is null then 0 else 1 end as isCleaning
        from bas_not_rented_out_source s
                 left join bas_not_rented_out o on o.source_id = s.source_id
    </sql>

    <select id="selectBasNotRentedOutSourceList" parameterType="BasNotRentedOutSource" resultMap="BasNotRentedOutSourceResult">
        <include refid="selectBasNotRentedOutSourceVo"/>
        <where>  
            <if test="propertiesSale != null  and propertiesSale != ''"> and s.properties_sale = #{propertiesSale}</if>
            <if test="building != null  and building != ''"> and s.building = #{building}</if>
            <if test="unit != null  and unit != ''"> and s.unit = #{unit}</if>
            <if test="roomNumber != null  and roomNumber != ''"> and s.room_number = #{roomNumber}</if>
            <if test="monthlyRent != null "> and s.monthly_rent = #{monthlyRent}</if>
            <if test="areaId != null  and areaId != ''"> and s.area_id = #{areaId}</if>
            <if test="importTime != null "> and s.import_time = #{importTime}</if>
        </where>
        order by s.create_time desc
    </select>
    
    <select id="selectBasNotRentedOutSourceById" parameterType="String" resultMap="BasNotRentedOutSourceResult">
        <include refid="selectBasNotRentedOutSourceVo"/>
        where s.source_id = #{sourceId}
    </select>
        
    <insert id="insertBasNotRentedOutSource" parameterType="BasNotRentedOutSource">
        insert into bas_not_rented_out_source
        <trim prefix="(" suffix=")" suffixOverrides=",">
