    <update id="updateBasNotRentedOutSource" parameterType="BasNotRentedOutSource">
        update bas_not_rented_out_source
        <trim prefix="SET" suffixOverrides=",">
            <if test="propertiesSale != null">properties_sale = #{propertiesSale},</if>
            <if test="building != null">building = #{building},</if>
            <if test="unit != null">unit = #{unit},</if>
            <if test="roomNumber != null">room_number = #{roomNumber},</if>
            <if test="monthlyRent != null">monthly_rent = #{monthlyRent},</if>
