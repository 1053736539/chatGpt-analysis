            <if test="updateBy != null">#{updateBy},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="projectName != null">#{projectName},</if>
         </trim>
    </insert>
    <insert id="insertBasNotRentedOutSourceBatch" parameterType="BasNotRentedOutSource">
