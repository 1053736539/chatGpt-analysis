             #{item.sourceId},#{item.propertiesSale},#{item.building},#{item.unit},#{item.roomNumber},#{item.monthlyRent},#{item.areaId},#{item.importTime},
            #{item.createTime},#{item.updateTime},#{item.createBy},#{item.updateBy},#{item.delFlag},#{item.projectName}
