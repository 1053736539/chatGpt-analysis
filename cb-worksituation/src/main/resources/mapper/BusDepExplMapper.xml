<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cb.worksituation.mapper.BusDepExplMapper">

    <resultMap type="BusDepExpl" id="BusDepExplResult">
        <result property="id"    column="ID"    />
        <result property="divisionDept"    column="DIVISION_DEPT"    />
        <result property="firstLevel"    column="FIRST_LEVEL"    />
        <result property="twoLevel"    column="TWO_LEVEL"    />
        <result property="score"    column="SCORE"    />
        <result property="evaluationDept"    column="EVALUATION_DEPT"    />
        <result property="createBy"    column="CREATE_BY"    />
        <result property="createTime"    column="CREATE_TIME"    />
        <result property="updateBy"    column="UPDATE_BY"    />
        <result property="updateTime"    column="UPDATE_TIME"    />
    </resultMap>

    <sql id="selectBusDepExplVo">
        select ID, DIVISION_DEPT, FIRST_LEVEL, TWO_LEVEL, SCORE, EVALUATION_DEPT, CREATE_BY, CREATE_TIME, UPDATE_BY, UPDATE_TIME from BUS_DEP_EXPL
    </sql>

    <select id="selectBusDepExplList" parameterType="BusDepExpl" resultMap="BusDepExplResult">
        <include refid="selectBusDepExplVo"/>
        <where>
            <if test="divisionDept != null  and divisionDept != ''"> and DIVISION_DEPT = #{divisionDept}</if>
            <if test="firstLevel != null  and firstLevel != ''"> and FIRST_LEVEL = #{firstLevel}</if>
            <if test="twoLevel != null  and twoLevel != ''"> and TWO_LEVEL = #{twoLevel}</if>
            <if test="score != null "> and SCORE = #{score}</if>
            <if test="evaluationDept != null  and evaluationDept != ''"> and EVALUATION_DEPT = #{evaluationDept}</if>
            <if test="createBy != null  and createBy != ''"> and CREATE_BY = #{createBy}</if>
            <if test="createTime != null "> and CREATE_TIME = #{createTime}</if>
            <if test="updateBy != null  and updateBy != ''"> and UPDATE_BY = #{updateBy}</if>
            <if test="updateTime != null "> and UPDATE_TIME = #{updateTime}</if>
            <!-- 数据范围过滤 -->
            ${params.dataScope}
        </where>
    </select>

    <select id="selectBusDepExplById" parameterType="String" resultMap="BusDepExplResult">
        <include refid="selectBusDepExplVo"/>
        where ID = #{id}
    </select>

    <insert id="insertBusDepExpl" parameterType="BusDepExpl">
        insert into BUS_DEP_EXPL
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null and id != ''">ID,</if>
            <if test="divisionDept != null">DIVISION_DEPT,</if>
            <if test="firstLevel != null">FIRST_LEVEL,</if>
            <if test="twoLevel != null">TWO_LEVEL,</if>
            <if test="score != null">SCORE,</if>
            <if test="evaluationDept != null">EVALUATION_DEPT,</if>
            <if test="createBy != null">CREATE_BY,</if>
            <if test="createTime != null">CREATE_TIME,</if>
            <if test="updateBy != null">UPDATE_BY,</if>
            <if test="updateTime != null">UPDATE_TIME,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null and id != ''">#{id},</if>
            <if test="divisionDept != null">#{divisionDept},</if>
            <if test="firstLevel != null">#{firstLevel},</if>
            <if test="twoLevel != null">#{twoLevel},</if>
            <if test="score != null">#{score},</if>
            <if test="evaluationDept != null">#{evaluationDept},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <insert id="insertBatch" keyProperty="id">
        insert into BUS_DEP_EXPL
        <trim prefix="(" suffix=")" suffixOverrides=",">
                            ID,
                            DIVISION_DEPT,
                            FIRST_LEVEL,
                            TWO_LEVEL,
                            SCORE,
                            EVALUATION_DEPT,
                            CREATE_BY,
                            CREATE_TIME,
                            UPDATE_BY,
                            UPDATE_TIME,
                    </trim>
        values
        <foreach collection="entities" item="entity" separator=",">
            <trim prefix="(" suffix=")" suffixOverrides=",">
                                    #{entity.id},
                                    #{entity.divisionDept},
                                    #{entity.firstLevel},
                                    #{entity.twoLevel},
                                    #{entity.score},
                                    #{entity.evaluationDept},
                                    #{entity.createBy},
                                    #{entity.createTime},
                                    #{entity.updateBy},
                                    #{entity.updateTime},
                            </trim>
        </foreach>
    </insert>

    <update id="updateBusDepExpl" parameterType="BusDepExpl">
        update BUS_DEP_EXPL
        <trim prefix="SET" suffixOverrides=",">
            <if test="divisionDept != null">DIVISION_DEPT = #{divisionDept},</if>
            <if test="firstLevel != null">FIRST_LEVEL = #{firstLevel},</if>
            <if test="twoLevel != null">TWO_LEVEL = #{twoLevel},</if>
            <if test="score != null">SCORE = #{score},</if>
            <if test="evaluationDept != null">EVALUATION_DEPT = #{evaluationDept},</if>
            <if test="createBy != null">CREATE_BY = #{createBy},</if>
            <if test="createTime != null">CREATE_TIME = #{createTime},</if>
            <if test="updateBy != null">UPDATE_BY = #{updateBy},</if>
            <if test="updateTime != null">UPDATE_TIME = #{updateTime},</if>
        </trim>
        where ID = #{id}
    </update>

    <delete id="deleteBusDepExplById" parameterType="String">
        delete from BUS_DEP_EXPL where ID = #{id}
    </delete>

    <delete id="deleteBusDepExplByIds" parameterType="String">
        delete from BUS_DEP_EXPL where ID in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
